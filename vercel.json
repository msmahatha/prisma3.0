{
    "version": 2,
    "framework": null,
    "outputDirectory": ".",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.2",
            "includeFiles": "api/**,app/**,bootstrap/**,config/**,routes/**,vendor/**,resources/views/**,public/index.php,public/.htaccess,public/build/manifest.json,artisan,composer.json"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Prisma",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "",
        "LOG_CHANNEL": "stderr",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_STORE": "array",
        "SESSION_DRIVER": "array",
        "QUEUE_CONNECTION": "sync"
    },
    "buildCommand": "npm install && npm run build"
}
