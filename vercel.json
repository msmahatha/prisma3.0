{
    "version": 2,
    "framework": null,
    "outputDirectory": ".",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.2",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Prisma",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "",
        "LOG_CHANNEL": "stderr",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "CACHE_STORE": "array",
        "SESSION_DRIVER": "array",
        "QUEUE_CONNECTION": "sync",
        "COMPOSER_NO_DEV": "1"
    },
    "buildCommand": "find vendor -type f \\( -name '*.md' -o -name '*.txt' -o -name 'CHANGELOG*' -o -name 'LICENSE*' -o -name '*.dist' -o -name '*.yml' -o -name '*.yaml' -o -name '*.xml' \\) -delete && find vendor -type d \\( -name 'tests' -o -name 'test' -o -name 'docs' -o -name 'doc' \\) -exec rm -rf {} + 2>/dev/null; true"
}
